#leetcode


class Solution:
    def uniqueMorseRepresentations(self, words):
        """
        :type words: List[str]
        :rtype: int
        """
        alphabet_list = list(map(chr,range(ord('a'),ord('z')+1)))
        morse_code = [".-","-...","-.-.","-..",".","..-.","--.","....","..",".---","-.-",".-..","--","-.","---",".--.","--.-",".-.","...","-","..-","...-",".--","-..-","-.--","--.."]
        reference = dict(zip(alphabet_list,morse_code))
        translation = []
        for word in words:
            s=""
            for letter in word:
                code = reference[letter]
                s = s + code
            translation.append(s)
        return (len(set(translation)))
